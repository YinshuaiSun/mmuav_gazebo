<?xml version="1.0" ?>

<!--
    mmuav_geometric_control.launch
    Created on: May 11, 2018
        Author: lmark
-->

<launch>
    
    <arg name="namespace" default="mmuav"/>
    <arg name="rate" default="100"/>
    
    <node name="geometric_control" pkg="mmuav_control" type="UavGeometryControl"
        output="screen">
      <param name="rate" value="(arg rate)"/>
    </node>
    
</launch>